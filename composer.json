{
    "name": "novalnet_pimcore/payment-provider-novalnet-payment-gateway",
    "license": "GPL-3.0+",
    "type": "pimcore-bundle",
    "version": "1.0.0",
    "description": "Pimcore Payment Provider - Novalnet Payment Gateway",
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "pimcore/pimcore": "^11.0",
        "pimcore/ecommerce-framework-bundle": "^1.0.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Pimcore\\Bundle\\EcommerceFrameworkBundle\\": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "Pimcore\\Bundle\\EcommerceFrameworkBundle\\PimcorePaymentProviderNovalnetPaymentGateway"
            ]
        }
    }
}
  
