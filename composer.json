{
    "name": "pimcore_novalnet/pimcore-novalnet-payment",
    "license": "GPL-3.0+",
    "type": "pimcore-bundle",
    "version": "1.0.0",
    "description": "Pimcore Payment Provider - Novalnet Payment",
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "pimcore/pimcore": "^11.0",
        "pimcore/ecommerce-framework-bundle": "^1.0.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Novalnet\\PimcoreNovalnetPaymentBundle\\":"src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "Novalnet\\PimcoreNovalnetPaymentBundle\\PimcoreNovalnetPayment"
            ]
        }
    }
}
  
